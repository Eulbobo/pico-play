pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
rabbit={}

rabbit.x = 64
rabbit.y = 64
rabbit.sp = 0
rabbit.d = true
rabbit.s = 0
rabbit.t = 0

position = 0

carrots={}
moles={}

score=0
score_mole=0

function draw_rabbit(r)
	spr(r.sp,r.x,r.y,2,2,r.d)
end

function draw_sprite(s)
 spr(s.sp,s.x,s.y)
end

function add_carrot()
	if (#carrots<10) then
	 carrot={
		 x=flr(10+rnd(100)),
		 y=flr(10+rnd(100)),
		 sp=33
	 }
	 
	 add(carrots,carrot)
	end
end

function add_mole()
 -- les taupes apparaissent
 -- sur les bords exterieurs
 -- et spawn toutes 
 -- les 10 carottes
 if (score%10>#moles) then
  mole={
	 	x=-10,
	 	y=-10,
	 	sp=48,
	 	s=0,
	 	dx=0,--destination
	 	dy=0
	 }
	 add(moles,mole)
 end
end

function check_player()
	cpt = #carrots
	while cpt > 0 do
		c=carrots[cpt]
	 d2x=abs(rabbit.x+14-(c.x+8))
	 d2y=abs(rabbit.y+14-(c.y+8))
	 if (d2y<8 and d2x<8) then
	 	del(carrots,c)
	 	score+=1
	 end 
	 cpt-=1
	end
end

function move_mole(m)
 -- determiner la carotte 
 -- la plus proche
 min_dist=2000
 cpt = #carrots
	while cpt > 0 do
		c=carrots[cpt]
		dx=abs(m.x-c.x)
	 dy=abs(m.y-c.y)
	 dist=dx*dx+dy*dy
	 if (dist<min_dist) then
	 	m.dx=c.x
	 	m.dy=c.y
	 	min_dist=dist
	 end 
	 cpt-=1
	end
 -- se diriger la cible
 m.x=m.x+sgn(m.dx-m.x)
 m.y=m.y+sgn(m.dy-m.y)
 
	m.sp+= 1
	if (m.sp>50) then
		m.sp=48
	end
end

function check_mole(m)
 cpt = #carrots
	while cpt > 0 do
		c=carrots[cpt]
		d_x=abs(m.x-c.x)
	 d_y=abs(m.y-c.y)
	 if (d_x<8 and d_x<8) then
	 	del(carrots,c)
	 	score_mole+=1
	 end 
	 cpt-=1
	end
end

function moving_rabbit(r,h,v)
 r.x += h
 r.y += v
 r.s += 1
 r.t = 10
 if (r.s%2==0) then 
	 r.sp += 2
	end
 if (h == -1) then
 	r.d = true
 else
 	r.d = false
 end
 
 if (r.sp > 6 or r.sp < 0) then
 	r.sp = 0
 end
 
end

function player_move()
 if	btn(0) then
 	moving_rabbit(rabbit,-1,0)
 end
 if	btn(1) then
 	moving_rabbit(rabbit,1,0)
 end
 if btn(3) then
  moving_rabbit(rabbit,0,1)
 end
 if btn(2) then
  moving_rabbit(rabbit,0,-1)
 end
  
 rabbit.t -= 1
 if (rabbit.t < 0) then
 	rabbit.t = 0
 	rabbit.sp = 0
 end
end

function _update()
 player_move()
 add_carrot()
 add_mole()
 foreach(moles,move_mole)
 foreach(moles,check_mole)
 check_player()
end

function _init()
	cls()
	cpt=0
	repeat
	 add_carrot()
	 cpt=cpt+1
	until cpt>9
end

function _draw()
	cls()
	map(0,0,0,0,16,16) -- draw background
	foreach(carrots, draw_sprite)
	foreach(moles, draw_sprite)
	draw_rabbit(rabbit)
	rectfill(0,0,20,8,0)
	print(score,2,2,11)
end
